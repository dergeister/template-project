    "mitt": "^3.0.1",
    "pinia": "^2.1.7",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primevue": "^3.39.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.6.4",
    "vue-router": "^4.2.5",
    "vue-the-mask": "^0.11.1"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@vitejs/plugin-vue": "^4.4.0",

{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es6",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@enums/*": ["./src/domain/enums/*"],
      "@models/*": ["./src/domain/models/*"],
      "@factories/*": ["./src/domain/factories/*"],
      "@data/*": [".src/domain/data/*"],
      "@common/*": ["./src/presentation/modules/_common/*"],
      "@home/*": ["./src/presentation/modules/home/*"],
      "@i18n/*": ["./src/presentation/i18n/*"],
      "@router/*": [".src/presentation/router/*"],
      "@middlewares/*": [".src/presentation/middlewares/*"],
      "@mixins/*": [".src/presentation/mixins/*"],
      "@api/*": [".src/infrastructure/api/*"],
      "@store/*": [".src/application/stores/*"],
      "@helpers/*": [".src/application/helpers/*"],
      "@services/*": [".src/application/services/*"],
    }
  }
}